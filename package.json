{
  "name": "angular-sandbox",
  "version": "0.0.1",
  "description": "Angular practice and example project.",
  "type": "module",
  "packageManager": "bun@1.1.0",
  "engines": {
    "node": ">=18.20.0",
    "bun": ">=1.1.0"
  },
  "scripts": {
    "build": "ng build",
    "clean": "git clean -Xfd",
    "lint": "prettier . --check --cache && eslint . --cache",
    "lint:fix": "prettier . --write --cache && eslint . --fix --cache",
    "ng": "ng",
    "serve": "ng serve --port 3000",
    "serve:ssr": "bun run build && node dist/angular-sandbox/server/server.mjs"
  },
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/cdk": "^18.1.4",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/platform-server": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@angular/ssr": "^18.1.4",
    "@fortawesome/angular-fontawesome": "^0.15.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "express": "^4.18.2",
    "preline": "^2.4.1",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.4",
    "@angular-eslint/eslint-plugin": "^18.2.0",
    "@angular/cli": "^18.1.4",
    "@angular/compiler-cli": "^18.1.0",
    "@types/eslint": "^8.56.10",
    "@types/express": "^4.17.17",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-rxjs": "^5.0.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.2"
  }
}
